+package com.meta.cj.day9;
+
+import java.util.ArrayList;
+import java.util.Arrays;
+import java.util.concurrent.ThreadLocalRandom;
+
+public class FindBiggestOfNNumbers {
+
+    public static void main(String[] args) {
+
+        int[] arr = new int[10];
+        for (int i = 0; i< arr.length; i++){
+            arr[i] = ThreadLocalRandom.current().nextInt(1,50);
+        }
+        System.out.println(Arrays.toString(arr));
+        int max = biggest(arr);
+        int min = smallest(arr);
+        int sum = sumOfElements(arr);
+        int primeCount = primeCount(arr);
+        int evenCount = evenCount(arr);
+        int oddCount = oddCount(arr);
+        int[] a = perfectCount(arr);
+        System.out.println("Biggest number in the array is " + max + " and Smallest is " + min);
+        System.out.println("The Sum of the numbers in the array is " + sum);
+        System.out.println("The number of prime numbers in this array : " + primeCount);
+        System.out.println("The number of even numbers in the array : " + evenCount);
+        System.out.println("The number of odd numbers in the array : " + oddCount);
+        System.out.println("The perfectCount numbers in the array :" + Arrays.toString(a));
+
+    }
+
+    private static int biggest(int[] numArr){
+        int max = 0;
+        for( int a : numArr){
+            if( a > max){
+                max = a;
+            }
+        }
+        return max;
+    }
+    private static int smallest(int[] numArr){
+        int min = numArr[0];
+        for( int a : numArr){
+            if( a < min){
+                min = a;
+            }
+        }
+        return min;
+    }
+    private static int sumOfElements(int[] numArr){
+        int sum = 0;
+        for( int a : numArr){
+            sum += a;
+        }
+        return sum;
+    }
+
+    private static int primeCount(int[] numArr){
+        int count = 0;
+        for( int a : numArr){
+            int factors = 0;
+            for( int i = 1; i <= a ; i++){
+                if(a % i == 0){
+                    factors++;
+                }
+            }
+            if(factors == 2){
+                count++;
+            }
+        }
+        return count;
+    }
+
+    private static int evenCount(int[] numArr){
+        int count = 0;
+        for( int a : numArr){
+            if( a % 2 == 0){
+                count++;
+            }
+        }
+        return count;
+    }
+    private static int oddCount(int[] numArr){
+        int count = 0;
+        for( int a : numArr){
+            if( a % 2 == 1){
+                count++;
+            }
+        }
+        return count;
+    }
+    private static int[] perfectCount(int[] numArr){
+        int n = numArr.length;
+        int count = 0;
+        int[] arr = new int[n];
+        for( int a : numArr){
+            int sum = 0;
+            for( int i=1; i <= (a/2); i++){
+                if( a % i == 0){
+                    sum += i;
+                }
+            }
+            if(sum == a){
+                arr[count] = a;
+                count++;
+            }
+        }
+        return arr;
+    }
+}
