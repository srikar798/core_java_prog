+package com.meta.cj.day11.pms;
+
+class ProductServices {
+
+    private Product[] arr;
+
+    public ProductServices() {
+        Product p1 = new Product(1001, "IPhone15", 125000);
+        Product p2 = new Product(1002, "IPhone14", 100000);
+        Product p3 = new Product(1003, "IPhone13", 95000);
+        Product p4 = new Product(1004, "Samsung", 80000);
+        Product p6 = new Product(1005, "IPhone13", 75000);
+        Product p7 = new Product(1006, "IPhone15pro", 125000);
+
+        arr = new Product[]{p1, p2, p3, p4, p6, p7};
+    }
+
+    public void showAllProducts() {
+        System.out.println("-".repeat(20));
+        for (Product p : arr) {
+            System.out.println(p.getPid() + " " + p.getPname() + " " + p.getPrice());
+        }
+        System.out.println("-".repeat(20));
+    }
+
+    public void showMaxPriceProducts() {
+        int maxPrice = getMaxPrice();
+        System.out.println("-".repeat(20));
+        for (Product p : arr) {
+            if (p.getPrice() == maxPrice) {
+                System.out.println(p.getPid() + " " + p.getPname() + " " + p.getPrice());
+            }
+        }
+        System.out.println("-".repeat(20));
+    }
+
+    public int getMaxPrice() {
+        int maxPrice = 0;
+        for (Product p : arr) {
+            if (p.getPrice() > maxPrice) {
+                maxPrice = p.getPrice();
+            }
+        }
+        return maxPrice;
+        //  System.out.println(" Max price product details " + maxPriceProduct.getPid() + " " + maxPriceProduct.getPname() + " " + maxPriceProduct.getPrice());
+
+    }
+
+    public void showMinPriceProducts() {
+
+        int minPrice = getMinPrice();
+        System.out.println("-".repeat(20));
+        for (Product p : arr) {
+            if (p.getPrice() == minPrice) {
+                System.out.println(p.getPid() + " " + p.getPname() + " " + p.getPrice());
+            }
+        }
+        System.out.println("-".repeat(20));
+
+    }
+
+    public int getMinPrice() {
+        int minPrice = arr[0].getPrice();
+        for (Product p : arr) {
+            if (p.getPrice() < minPrice) {
+                minPrice = p.getPrice();
+            }
+        }
+
+        return minPrice;
+    }
+
+    public void search(String str) {
+        System.out.println("-".repeat(20));
+        for (Product p : arr) {
+            if (p.getPname().toLowerCase().contains(str.toLowerCase())) {
+                System.out.println(p.getPid() + " " + p.getPname() + " " + p.getPrice());
+            }
+        }
+        System.out.println("-".repeat(20));
+    }
+
+}
